{
  "custom": {},
  "params": {
    "format": "",
    "label": "",
    "path": "",
    "type": "",
    "units": ""
  },
  "propConfig": {
    "params.format": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.path": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.type": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.units": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 25,
      "width": 400
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "200px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "{view.params.label}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "fontSize": 12,
            "textAlign": "right"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "LedDisplay"
        },
        "position": {
          "basis": "150px",
          "grow": 1
        },
        "propConfig": {
          "props.numberFormat": {
            "binding": {
              "config": {
                "path": "view.params.format"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "path": "{view.params.path}"
                },
                "tagPath": "{path}"
              },
              "transforms": [
                {
                  "expression": "if( {view.params.type} \u003d \"Boolean\",\r\n\tif({value}, \"ON\", \"OFF\"),\r\n\t{value}\r\n)",
                  "type": "expression"
                }
              ],
              "type": "tag"
            }
          }
        },
        "type": "ia.display.led-display"
      },
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "basis": "80px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.units"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "textAlign": "left"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Base/Black"
      }
    },
    "type": "ia.container.flex"
  }
}