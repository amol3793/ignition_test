{
  "custom": {},
  "params": {},
  "propConfig": {
    "params.feature": {
      "paramDirection": "input"
    },
    "params.key": {
      "paramDirection": "input"
    },
    "params.tier1": {
      "paramDirection": "input"
    },
    "params.tier2": {
      "paramDirection": "input"
    }
  },
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "name": "Title"
        },
        "position": {
          "basis": "56px",
          "shrink": 0
        },
        "props": {
          "params": {
            "icon": "alarm",
            "text": "Alarms"
          },
          "path": "Page/Embedded/Title"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "AlarmStatusTable"
        },
        "position": {
          "basis": "400px",
          "grow": 1
        },
        "propConfig": {
          "props.enableAcknowledge": {
            "binding": {
              "config": {
                "expression": "hasRole(\"autogrid\", {session.props.auth.user.userName}, \"default\") || hasRole(\"operator\", {session.props.auth.user.userName}, \"default\")"
              },
              "enabled": false,
              "type": "expr"
            }
          },
          "props.enableShelve": {
            "binding": {
              "config": {
                "expression": "hasRole(\"autogrid\", {session.props.auth.user.userName}, \"default\") || hasRole(\"operator\", {session.props.auth.user.userName}, \"default\")"
              },
              "type": "expr"
            }
          },
          "props.enableUnshelve": {
            "binding": {
              "config": {
                "expression": "hasRole(\"autogrid\", {session.props.auth.user.userName}, \"default\") || hasRole(\"operator\", {session.props.auth.user.userName}, \"default\")"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "columns": {
            "active": {
              "ackTime": {
                "enabled": true
              },
              "ackUser": {
                "enabled": true
              },
              "clearTime": {
                "enabled": true
              },
              "displayPath": {
                "enabled": false
              },
              "label": {
                "enabled": true
              }
            }
          }
        },
        "type": "ia.display.alarmstatustable"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Page/Page"
      }
    },
    "type": "ia.container.flex"
  }
}